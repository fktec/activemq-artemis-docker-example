<addresses xmlns="urn:activemq:core">

	<!-- Default -->
	<address name="DLQ">
		<anycast>
			<queue name="DLQ" />
		</anycast>
	</address>
	<address name="ExpiryQueue">
		<anycast>
			<queue name="ExpiryQueue" />
		</anycast>
	</address>

	<!-- Topic/Queue - Teste 1 -->
	<address name="test.temporary">
		<multicast>
            <queue name="sub.t1.test.temporary">
				<filter string="xQueueTarget IS NULL OR xQueueTarget='TargetOne'" />
			</queue>
			<queue name="sub.t2.test.temporary">
				<filter string="xQueueTarget IS NULL OR xQueueTarget='TargetTwo'" />
			</queue>
			<queue name="sub.t3.test.temporary">
				<filter string="xQueueTarget IS NULL" />
			</queue>
			<queue name="sub.t4.test.temporary">
				<filter string="xQueueTarget IS NULL OR xQueueTarget IS false" />
			</queue>
			<queue name="sub.t5.test.temporary">
				<filter string="xQueueTarget IS NULL OR xQueueTarget='TargetFive1' OR xQueueTarget='TargetFive2'" />
			</queue>
        </multicast>
    </address>
    <address name="test.temporary.DLA">
		<anycast>
			<queue name="sub.t1.test.temporary.DLQ">
				<durable>true</durable>
				<filter string="_AMQ_ORIG_QUEUE='sub.t1.test.temporary'" />
			</queue>
			<queue name="sub.t2.test.temporary.DLQ">
				<durable>true</durable>
				<filter string="_AMQ_ORIG_QUEUE='sub.t2.test.temporary'" />
			</queue>
			<queue name="sub.t3.test.temporary.DLQ">
				<durable>true</durable>
				<filter string="_AMQ_ORIG_QUEUE='sub.t3.test.temporary'" />
			</queue>
			<queue name="sub.t4.test.temporary.DLQ">
				<durable>true</durable>
				<filter string="_AMQ_ORIG_QUEUE='sub.t4.test.temporary'" />
			</queue>
			<queue name="sub.t5.test.temporary.DLQ">
				<durable>true</durable>
				<filter string="_AMQ_ORIG_QUEUE='sub.t5.test.temporary'" />
			</queue>
		</anycast>
	</address>

</addresses>

